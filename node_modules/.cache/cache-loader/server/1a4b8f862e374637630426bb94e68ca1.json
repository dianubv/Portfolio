.target.selectionEnd = event.target.selectionStart =\n        originalSelectionStart + 1\n    },\n    onTabLeft(event) {\n      if (this.isInComposition) {\n        return\n      }\n      const text = this.file\n      const originalSelectionStart = event.target.selectionStart\n      const textStart = text.slice(0, originalSelectionStart)\n      const textEnd = text.slice(originalSelectionStart)\n      this.file = `${textStart.replace(/\\t$/, '')}${textEnd}`\n      event.target.value = this.file // required to make the cursor stay in place.\n      event.target.selectionEnd = event.target.selectionStart =\n        originalSelectionStart - 1\n    }\n  }\n}\n"],"mappings":"AAaA,eAAe;AAAA,EACb,OAAO;AAAA,IACL,OAAO;AAAA,IACP,WAAW;AAAA,EACb;AAAA,EACA,OAAQ;AACN,WAAO;AAAA,MACL,MAAM;AAAA,MACN,iBAAiB;AAAA,IACnB;AAAA,EACF;AAAA,EACA,OAAO;AAAA,IACL,QAAS;AACP,WAAK,OAAO,KAAK;AAAA,IACnB;AAAA,IACA,YAAa;AACX,WAAK,OAAO;AACZ,WAAK,MAAM,SAAS,MAAM;AAAA,IAC5B;AAAA,IACA,OAAQ;AACN,WAAK,OAAO;AACZ,WAAK,MAAM,SAAS,KAAK,IAAI;AAAA,IAC/B;AAAA,EACF;AAAA,EACA,SAAS;AAAA,IACP,SAAU;AACR,YAAM,KAAK,KAAK,MAAM;AAEtB,SAAG,MAAM,SAAS;AAClB,WAAK,UAAU,MAAM;AACnB,WAAG,MAAM,SAAS,GAAG,eAAe;AAAA,MACtC,CAAC;AAAA,IACH;AAAA,IACA,WAAW,OAAO;AAChB,UAAI,KAAK,iBAAiB;AACxB;AAAA,MACF;AACA,YAAM,OAAO,KAAK;AAClB,YAAM,yBAAyB,MAAM,OAAO;AAC5C,YAAM,YAAY,KAAK,MAAM,GAAG,sBAAsB;AACtD,YAAM,UAAU,KAAK,MAAM,sBAAsB;AACjD,WAAK,OAAO,GAAG,aAAc;AAC7B,YAAM,OAAO,QAAQ,KAAK;AAC1B,YAAM,OAAO,eAAe,MAAM,OAAO,iBACvC,yBAAyB;AAAA,IAC7B;AAAA,IACA,UAAU,OAAO;AACf,UAAI,KAAK,iBAAiB;AACxB;AAAA,MACF;AACA,YAAM,OAAO,KAAK;AAClB,YAAM,yBAAyB,MAAM,OAAO;AAC5C,YAAM,YAAY,KAAK,MAAM,GAAG,sBAAsB;AACtD,YAAM,UAAU,KAAK,MAAM,sBAAsB;AACjD,WAAK,OAAO,GAAG,UAAU,QAAQ,OAAO,EAAE,IAAI;AAC9C,YAAM,OAAO,QAAQ,KAAK;AAC1B,YAAM,OAAO,eAAe,MAAM,OAAO,iBACvC,yBAAyB;AAAA,IAC7B;AAAA,EACF;AACF;","names":[]}]}